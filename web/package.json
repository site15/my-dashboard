{
  "name": "my-dashboard",
  "version": "0.0.26",
  "description": "A fullstack web application built with AnalogJS (Angular-based meta-framework) featuring Telegram authentication, Prisma ORM for database management, and tRPC for API communication. The application is deployed on Vercel with Neon PostgreSQL database integration.",
  "author": "Ilshat Khamitov <ilshat.m.khamitov@gmail.com>",
  "homepage": "https://site15-my-dashboard.vercel.app",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "pkg": {
    "assets": [
      "node_modules/.prisma/client/*.node"
    ]
  },
  "scripts": {
    "ng": "ng",
    "dev": "vite",
    "start": "npm run dev",
    "build": "npx prisma generate && sed -i 's/process/processLocal/g' ./src/app/generated/prisma/client.ts && sed -i 's/process/processLocal/g' ./src/server/generated/prisma/client.ts && sed -i 's/node:processLocal/node:process/g' ./src/app/generated/prisma/client.ts && sed -i 's/node:processLocal/node:process/g' ./src/server/generated/prisma/client.ts && vite build",
    "watch": "vite build --watch",
    "test": "vitest",
    "preview": "node dist/analog/server/index.mjs"
  },
  "private": true,
  "dependencies": {
    "@analogjs/content": "^1.22.4",
    "@analogjs/router": "^1.22.4",
    "@analogjs/trpc": "^0.4.0",
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@prisma/adapter-pg": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "@tailwindcss/vite": "^4.1.4",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "isomorphic-fetch": "^3.0.0",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.4",
    "tslib": "^2.3.0",
    "vite-plugin-static-copy": "^3.1.4",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.22.4",
    "@analogjs/vite-plugin-angular": "^1.22.4",
    "@analogjs/vitest-angular": "^1.22.4",
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/node": "^24.9.2",
    "jsdom": "^22.0.0",
    "prisma": "^6.18.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^4.0.0"
  },
  "overrides": {
    "vitest": "$vitest"
  }
}
